{
    "name": "videoclipper-v2",
    "version": "1.0.0",
    "scripts": {
        "build": "npm run build:frontend && npm run build:backend",
        "build:frontend": "webpack --config frontend/webpack.config.js",
        "build:backend": "tsc -p backend/tsconfig.json",
        "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
        "dev:frontend": "webpack serve --config frontend/webpack.config.js",
        "dev:backend": "ts-node-dev --respawn --transpile-only --project backend/tsconfig.json backend/src/index.ts",
        "start": "node backend/dist/index.js",
        "lint": "eslint . --ext .ts"
    },
    "dependencies": {
        "@ffmpeg/ffmpeg": "^0.12.10",
        "@ffmpeg/types": "^0.12.2",
        "@ffmpeg/util": "^0.12.1",
        "@types/d3": "^7.4.3",
        "@types/lodash": "^4.17.5",
        "@ungap/structured-clone": "^1.2.0",
        "concurrently": "^8.2.2",
        "cookie-parser": "^1.4.7",
        "cors": "^2.8.5",
        "d3": "^7.9.0",
        "dom-to-image-more": "^3.3.0",
        "dotenv": "^16.5.0",
        "express": "^4.21.2",
        "express-session": "^1.18.1",
        "google-auth-library": "^9.15.1",
        "html2canvas": "^1.4.1",
        "idb": "^8.0.2",
        "lodash": "^4.17.21",
        "lodash.debounce": "^4.0.8",
        "multer": "^1.4.5-lts.1",
        "paper": "^0.12.17",
        "passport": "^0.7.0",
        "passport-google-oauth20": "^2.0.0",
        "turbodombuilder": "0.8.84",
        "ws": "^8.18.1",
        "y-indexeddb": "^9.0.12",
        "y-leveldb": "^0.1.2",
        "y-webrtc": "^10.3.0",
        "y-websocket": "^3.0.0",
        "yjs": "^13.6.15"
    },
    "devDependencies": {
        "@eslint/js": "^8.57.0",
        "@types/cookie-parser": "^1.4.8",
        "@types/cors": "^2.8.17",
        "@types/eslint__js": "^8.42.3",
        "@types/express": "^4.17.17",
        "@types/lodash.debounce": "^4.0.9",
        "@types/multer": "^1.4.7",
        "@types/node": "^20.5.0",
        "@types/paper": "^0.12.3",
        "@types/ws": "^8.18.1",
        "cross-env": "^7.0.3",
        "css-loader": "^7.1.1",
        "eslint": "^8.57.0",
        "globals": "^15.6.0",
        "style-loader": "^4.0.0",
        "terser-webpack-plugin": "^5.3.10",
        "ts-loader": "^9.5.1",
        "ts-node": "^10.9.1",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.5.2",
        "typescript-eslint": "^7.14.1",
        "webpack": "^5.91.0",
        "webpack-cli": "^5.1.4",
        "webpack-dev-server": "^5.0.4"
    }
}
